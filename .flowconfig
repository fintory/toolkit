[ignore]
.*/test/.*
.*/internals/server/.*
.*/**/*.test.js

[include]

[libs]
flow-typed

[options]
esproposal.decorators=ignore
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe

module.ignore_non_literal_requires=true

module.name_mapper='\(bmp\|gif\|ico\|jpg\|jpeg\|png\|psd\|svg\|webp\|m4v\|mov\|mp4\|mpeg\|mpg\|webm\|aac\|aiff\|caf\|m4a\|mp3\|wav\|html\|pdf\)' -> 'empty/object'
